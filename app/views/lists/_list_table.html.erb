<table class="table table-light table-striped-columns">
  <thead>
    <tr>
      <th>name</th>
      <th>number</th>
      <th>email</th>
      <th>action</th>
    </tr>
  </thead>
  <tbody>
    <% @lists.each do |list| %>
      <tr>
        <td><%= list.name %></td>
        <td><%= list.number %></td>
        <td><%= list.email %></td>
        <td>
        <button type="button" class="btn btn-dark">
          <%= link_to "Edit", edit_list_path(list), class:"nav-link active" %>
          </button>
          <button type="button" class="btn btn-dark">
          <%= button_to  'destroy', method: :delete, class:"nav-link active" %>

          </button>

          </td>
      </tr>
    <% end %>
  </tbody>
</table>

